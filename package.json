{
  "name": "hacktoberfest-finder",
  "version": "1.0.0",
  "description": "Hacktoberfest Finder helps you find issues which you could help fix.",
  "keywords": [
    "Hacktoberfest",
    "Programming",
    "GitHub",
    "DigitalOcean",
    "DEV"
  ],
  "private": true,
  "main": "./public/index.html",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "homepage": "https://hacktoberfest-finder.netlify.com",
  "author": {
    "name": "Duncan McClean",
    "url": "http://duncan.mcclean.co.uk"
  },
  "repository": "github:damcclean/hacktoberfest-finder",
  "contributors": [
    {
      "name": "Adrian Mejias",
      "email": "adrianmejias@gmail.com",
      "url": "https://adrianmejias.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "and": "0.0.3",
    "cache": "^2.1.0",
    "laravel-mix": "^5.0.0",
    "laravel-mix-purgecss": "^4.2.0",
    "moment": "^2.24.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "cross-env": "6.0.3",
    "laravel-mix-postcss-config": "^1.0.2",
    "postcss-nested": "^4.1.2",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "tailwindcss": "^1.1.2",
    "vue-click-outside": "^1.0.7",
    "vue-fixed-header": "^3.2.2",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

{
  "name": "hacktoberfest-finder",
  "version": "1.0.0",
  "description": "Hacktoberfest Finder helps you find issues which you could help fix.",
  "keywords": [
    "Hacktoberfest",
    "Programming",
    "GitHub",
    "DigitalOcean",
    "DEV"
  ],
  "private": true,
  "main": "./public/index.html",
  "scripts": {

    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "and": "0.0.3",
    "cache": "^2.1.0",
    "laravel-mix": "^6.0",
    "mitt": "^3.0.1",
    "moment": "^2.24.0",
    "vue": "^3.0.0",
